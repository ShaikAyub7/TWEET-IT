{
  "version": 2,
  "builds": [
    {
      "src": "app/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "app/static/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app/wsgi.py"
    }
  ],
  "build": {
    "env": {
      "DJANGO_COLLECTSTATIC": "True",
      "DJANGO_SETTINGS_MODULE": "app.settings",
