{
  "version": 2,
  "builds": [
    {
      "src": "app/wsgi.py",
      "use": "@vercel/python"
    },
    {
      "src": "app/static/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "app/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "app.settings",
    "DATABASE_URL": "postgres://tweet_it_vohg_user:XwH911nTH9fhI31k0u1FbeUMgK8BMJRn@dpg-cqeg71o8fa8c73e6ec90-a.oregon-postgres.render.com/tweet_it_vohg",
    "DJANGO_COLLECTSTATIC": "true"
  }
}
